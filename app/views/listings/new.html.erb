	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>


<%= form_for @listing, :url => listings_path, :html => { :multipart => true } do |f| %>
	<%= f.label(:name) %>
	<%=f.text_field :name %>
	<%= f.label :price %>
	<%= f.number_field :price %>
  	<%= f.file_field :avatar %>
  	<%=f.label :size %>
  	<input type="text" name="listing[size]" />
	<%=f.submit %>

	<% end %>

