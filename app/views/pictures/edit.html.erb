
  <h1 class="h1-headers">Edit Picture</h1>


<%= form_for @picture do |f| %>

    <%= image_tag @picture.image.url %><br>
    <%= f.label :description %>

    <%= f.text_field :description%><br>

    <%= f.label :size %>

    <%= f.text_field :size%><br>    


  
 
    <%= f.submit  %>


<% end %>

