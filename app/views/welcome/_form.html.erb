	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>


<%= form_for @listing, :url => listings_path, :html => { :multipart => true }, remote: true do |f| %>
	<%= f.label(:name) %>
	<%=f.text_field :name %>

 <%= file_field_tag "images[]", type: :file, multiple: true %>
   	<%=f.label :size %>
   	<%=f.text_field :size %>
  	<%=f.label :description %>
	<%=f.text_field :description %>
	<%=f.label :section %>
<%= f.select :section, options_for_select([["Select Type", ""], "Gondola and Accessories", "Mannequin", "Pharmacy Display", "Security Mirror", "Shopping Carts and Baskets", "Showcase", "Slatwall and Accessories", "Tags and Guns", "Wire Basket", "Hangers", "Racks", "Gridwall"]) %>	

	<%=f.submit %>

	<% end %>

