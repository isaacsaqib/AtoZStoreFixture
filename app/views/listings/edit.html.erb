<h1>Editing article</h1>
 
<%= form_for :listing, url: listing_path(@listing), method: :patch do |f| %>
 
    <%= f.label :name %><br>
    <%= f.text_field :name %>
	 <%= f.label :price %>
	<%= f.number_field :price %>
	<%= f.label :image_url %>
	<%= f.text_field :image_url %>


    <%= f.submit %>
 
<% end %>
 
