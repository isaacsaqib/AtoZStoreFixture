<h1>Editing article</h1>
 
<%= form_for :listing, url: listing_path(@listing), method: :patch do |f| %>
 
    <%= f.label :name %><br>
    <%= f.text_field :name %>
	<%= f.label :price %>
	<%= f.number_field :price %>
	<%= f.file_field :avatar %>

<%=f.label :size %>
  	<%= f.text_field :size %>

    <%= f.submit %>
 
<% end %>
 
